{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "koa"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prefer-stateless-function": 0
  }
}
